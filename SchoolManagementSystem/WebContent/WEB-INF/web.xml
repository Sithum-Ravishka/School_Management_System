<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
		<servlet-name>Result_Insert</servlet-name>
        <servlet-class>com.school.Result_Insert</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>Result_Insert</servlet-name>
    	<url-pattern>/reinsert</url-pattern>
    </servlet-mapping>
    
    <servlet>
		<servlet-name>ViewResultServlet</servlet-name>
        <servlet-class>com.school.ViewResultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>ViewResultServlet</servlet-name>
    	<url-pattern>/view</url-pattern>
    </servlet-mapping>
    
      <servlet>
		<servlet-name>ViewResultTeacherServlet</servlet-name>
        <servlet-class>com.school.ViewResultTeacherServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>ViewResultTeacherServlet</servlet-name>
    	<url-pattern>/viewTeacher</url-pattern>
    </servlet-mapping>
         
    <servlet>
		<servlet-name>UpdateResult</servlet-name>
        <servlet-class>com.school.ResultUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>UpdateResult</servlet-name>
    	<url-pattern>/update</url-pattern>
    </servlet-mapping>     
    
    <servlet>
		<servlet-name>DeleteResult</servlet-name>
        <servlet-class>com.school.DeleteResultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>DeleteResult</servlet-name>
    	<url-pattern>/delete</url-pattern>
    </servlet-mapping>
</web-app>